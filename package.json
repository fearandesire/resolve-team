{
	"name": "resolve-team",
	"version": "1.2.2",
	"description": "Resolve NBA & NFL sports team names via fuzzy search (FuseJS)",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"prepublish": "tsc",
		"test": "jest"
	},
	"keywords": [
		"nba",
		"nfl",
		"sports",
		"resolve",
		"team"
	],
	"author": "fearandesire",
	"license": "MIT",
	"dependencies": {
		"fuse.js": "^6.6.2",
		"typescript": "^5.0.2"
	},
	"devDependencies": {
		"@babel/core": "^7.22.11",
		"@babel/preset-env": "^7.22.10",
		"@babel/preset-typescript": "^7.22.11",
		"@types/jest": "^29.5.11",
		"@types/node": "^20.5.4",
		"babel-jest": "^29.6.4",
		"jest": "^29.7.0",
		"ts-jest": "^29.1.1"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
